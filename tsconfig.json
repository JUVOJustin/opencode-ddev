{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "lib": ["ES2020"],
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "types": ["node"]
    },
    "include": [
        "index.ts",
        "logs.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}